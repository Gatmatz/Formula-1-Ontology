@prefix : <http://example.org/formula1#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spin: <http://spinrdf.org/spin#> .
@base <http://example.org/formula1#> .

<http://example.org/formula1> rdf:type owl:Ontology ;
                               spin:imports <http://topbraid.org/spin/rdfsplus> ;
                               owl:versionInfo "Created with TopBraid Composer" .

#################################################################
#    Annotation properties
#################################################################

###  http://spinrdf.org/spin#imports
spin:imports rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/formula1#achieved_by
:achieved_by rdf:type owl:ObjectProperty ;
             owl:inverseOf :achievesPodium ;
             rdfs:domain :PodiumPlace ;
             rdfs:range :RacingDriver ;
             rdfs:label "Indicates the Driver that achieved a specific podium" .


###  http://example.org/formula1#achieved_in
:achieved_in rdf:type owl:ObjectProperty ;
             rdfs:domain :PodiumPlace ;
             rdfs:range :Event ;
             rdfs:label "Indicates the location of a specific Podium" .


###  http://example.org/formula1#achievesPodium
:achievesPodium rdf:type owl:ObjectProperty ;
                rdfs:label "Indicates a Podium is achieved by a specific Driver." .


###  http://example.org/formula1#attends
:attends rdf:type owl:ObjectProperty ;
         rdfs:domain :Viewer ;
         rdfs:range :RaceWeekend ;
         rdfs:label "Indicates the watching of a Grand Prix from a viewer" .


###  http://example.org/formula1#board_member
:board_member rdf:type owl:ObjectProperty ;
              rdfs:domain :Steward ;
              rdfs:range :RaceWeekend ;
              rdfs:label "Indicates the board members of stewards in a GP" .


###  http://example.org/formula1#broadcasts_for
:broadcasts_for rdf:type owl:ObjectProperty ;
                owl:inverseOf :hasJournalist ;
                rdfs:domain :PressMember ;
                rdfs:range :Channel ;
                rdfs:label "Indicates the work of a PressMember to a specific Broadcast Channel" .


###  http://example.org/formula1#buysTicket
:buysTicket rdf:type owl:ObjectProperty ;
            rdfs:domain :Viewer ;
            rdfs:range :Ticket ;
            rdfs:label "Indicates the purchase of a ticket for a Formula 1 Grand Prix" .


###  http://example.org/formula1#calls_for
:calls_for rdf:type owl:ObjectProperty ;
           rdfs:domain :Accident ;
           rdfs:range :Car ;
           rdfs:label "Indicates the use of a specific SafetyCar in an accident" .


###  http://example.org/formula1#caused_by
:caused_by rdf:type owl:ObjectProperty ;
           owl:inverseOf :hadAccident ;
           rdfs:domain :Accident ;
           rdfs:range :RacingDriver ;
           rdfs:label "Indicates the Drivers involves in a F1 Crash" .


###  http://example.org/formula1#caused_in
:caused_in rdf:type owl:ObjectProperty ;
           rdfs:domain :Accident ;
           rdfs:range :Event ;
           rdfs:label "Indicates the event that a crash/accident has occured." .


###  http://example.org/formula1#communicates_with
:communicates_with rdf:type owl:ObjectProperty ;
                   rdfs:domain :RacingDriver ;
                   rdfs:range :RaceEngineer ;
                   rdfs:label "Indicates the relationship between a racing Driver and his race enginner" .


###  http://example.org/formula1#competes_with
:competes_with rdf:type owl:ObjectProperty ;
               owl:inverseOf :developedBy ;
               rdf:type owl:InverseFunctionalProperty ;
               rdfs:domain :Team ;
               rdfs:range :RacingCar ;
               rdfs:label "Indicates the racing car a team is using" .


###  http://example.org/formula1#consists_of
:consists_of rdf:type owl:ObjectProperty ;
             rdfs:domain :Championship ;
             rdfs:range :RaceWeekend ;
             rdfs:label "Indicates the schedule of a F1 World Championship" .


###  http://example.org/formula1#constructor_champion
:constructor_champion rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :participated_in ;
                      rdfs:domain :Team ;
                      rdfs:range :Championship ;
                      rdfs:label "Indicates that a F1 Team won a specific World Constructors Championship" .


###  http://example.org/formula1#developedBy
:developedBy rdf:type owl:ObjectProperty ;
             rdfs:label "Indicates the development of a Racing Car from specific Team." .


###  http://example.org/formula1#driver_champion
:driver_champion rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :raced_in ;
                 rdfs:domain :RacingDriver ;
                 rdfs:range :Championship ;
                 rdfs:label "Indicates that a Racing Driver won a specific World Drivers Championship" .


###  http://example.org/formula1#employed_by
:employed_by rdf:type owl:ObjectProperty ;
             rdfs:domain :TeamMember ;
             rdfs:range :Team ;
             rdfs:label "Indicates the work of a TeamMember to a specific Team" .


###  http://example.org/formula1#hadAccident
:hadAccident rdf:type owl:ObjectProperty ;
             rdfs:label "Indicates that a Racing Driver has a specific Accident" .


###  http://example.org/formula1#hasCommonEngineSupplier
:hasCommonEngineSupplier rdf:type owl:ObjectProperty ,
                                  owl:SymmetricProperty ,
                                  owl:TransitiveProperty ;
                         rdfs:domain :Team ;
                         rdfs:range :Team ;
                         rdfs:label "Indicates that two Team have the same engine supplier." .


###  http://example.org/formula1#hasJournalist
:hasJournalist rdf:type owl:ObjectProperty ;
               rdfs:label "Indicates that a Press Channel are employing a Press Member." .


###  http://example.org/formula1#hasSimilarWeatherConditions
:hasSimilarWeatherConditions rdf:type owl:ObjectProperty ,
                                      owl:SymmetricProperty ,
                                      owl:TransitiveProperty ;
                             rdfs:domain :Event ;
                             rdfs:range :Event ;
                             rdfs:label "Indicates that two events have similar or same weather conditions." .


###  http://example.org/formula1#has_engine
:has_engine rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Car ;
            rdfs:range :Engine ;
            rdfs:label "Indicates the engine of a car" .


###  http://example.org/formula1#has_event
:has_event rdf:type owl:ObjectProperty ;
           rdfs:domain :RaceWeekend ;
           rdfs:range :Event ;
           rdfs:label "Indicates the specific events of a Grand Prix" .


###  http://example.org/formula1#haveSameNationality
:haveSameNationality rdf:type owl:ObjectProperty ,
                              owl:SymmetricProperty ,
                              owl:TransitiveProperty ;
                     rdfs:domain :RacingDriver ;
                     rdfs:range :RacingDriver ;
                     rdfs:label "Indicates that two Racing Drivers have the same nationality" .


###  http://example.org/formula1#isOrganizedBy
:isOrganizedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf :organises ;
               rdfs:label "Indicates that a Grand Prix is organised by a Company." .


###  http://example.org/formula1#isSponsoredBy
:isSponsoredBy rdf:type owl:ObjectProperty ;
               owl:inverseOf :sponsors ;
               rdfs:label "Indicates the sponsorship of F1 Team from a commercial thrid party company." .


###  http://example.org/formula1#isTeamMateWith
:isTeamMateWith rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain :RacingDriver ;
                rdfs:range :RacingDriver ;
                rdfs:label "Indicates that a specific RacingDriver is teammate with another RacingDriver." .


###  http://example.org/formula1#is_given_to
:is_given_to rdf:type owl:ObjectProperty ;
             rdfs:domain :Engine ;
             rdfs:range :RacingCar ;
             rdfs:label "Indicates that a Engine is given to a specific Racing Car." .


###  http://example.org/formula1#is_managing
:is_managing rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:InverseFunctionalProperty ;
             rdfs:domain :TeamPrincipal ;
             rdfs:range :Team ;
             rdfs:label "Indicates that a Team Principal is managing a Team." .


###  http://example.org/formula1#is_official
:is_official rdf:type owl:ObjectProperty ;
             rdfs:domain :RaceOfficial ;
             rdfs:range :FIA ;
             rdfs:label "Indicates that a RaceOfficial works for FIA in a specific year" .


###  http://example.org/formula1#needs_medical
:needs_medical rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :calls_for ;
               rdfs:domain :Accident ;
               rdfs:range :MedicalCar ;
               rdfs:label "Indicates the use of a MedicalCar in an F1 accident" .


###  http://example.org/formula1#organises
:organises rdf:type owl:ObjectProperty ;
           rdfs:domain :Company ;
           rdfs:range :RaceWeekend ;
           rdfs:label "Indicates the sponsorship of a company for a F1 Grand Prix" .


###  http://example.org/formula1#participated_in
:participated_in rdf:type owl:ObjectProperty ;
                 rdfs:domain :Team ;
                 rdfs:range :Championship ;
                 rdfs:label "Indicates that a F1 Team participated in a specific F1 World Championship" .


###  http://example.org/formula1#raced_in
:raced_in rdf:type owl:ObjectProperty ;
          rdfs:domain :RacingDriver ;
          rdfs:range :Championship ;
          rdfs:label "Indicates the participation of a Driver in a F1 World Championship" .


###  http://example.org/formula1#sponsors
:sponsors rdf:type owl:ObjectProperty ;
          rdfs:domain :Company ;
          rdfs:range :Team ;
          rdfs:label "Indicates the sponsorship of a commercial company to a F1 Team" .


###  http://example.org/formula1#takes_place
:takes_place rdf:type owl:ObjectProperty ;
             rdfs:domain :RaceWeekend ;
             rdfs:range :Circuit ;
             rdfs:label "Determines the location of a F1 GP" .


###  http://example.org/formula1#works_with
:works_with rdf:type owl:ObjectProperty ;
            rdfs:domain :Mechanic ;
            rdfs:range :Mechanic ;
            rdfs:label "Indicates that a specific Mechanic works with another Mechanic" .


#################################################################
#    Data properties
#################################################################

###  http://example.org/formula1#accidentDescription
:accidentDescription rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Accident ;
                     rdfs:range xsd:string ;
                     rdfs:label "A small description of a racing accident" .


###  http://example.org/formula1#board_year
:board_year rdf:type owl:DatatypeProperty ;
            rdfs:domain :FIA ;
            rdfs:range xsd:integer ;
            rdfs:label "Indicates the year of the FIA board" .


###  http://example.org/formula1#broadcaster
:broadcaster rdf:type owl:DatatypeProperty ;
             rdfs:domain :Channel ;
             rdfs:range xsd:string ;
             rdfs:label "The name of a F1 broadcast channel" .


###  http://example.org/formula1#cylinders
:cylinders rdf:type owl:DatatypeProperty ;
           rdfs:domain :Engine ;
           rdfs:range xsd:integer ;
           rdfs:label "The number of cylinders of an Engine" .


###  http://example.org/formula1#dateOfBirth
:dateOfBirth rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Person ;
             rdfs:range xsd:date ;
             rdfs:label "The date of brith of a Driver" .


###  http://example.org/formula1#engineModel
:engineModel rdf:type owl:DatatypeProperty ;
             rdfs:domain :Engine ;
             rdfs:range xsd:string ;
             rdfs:label "The model of a specific engine" .


###  http://example.org/formula1#fuelFlowLimit
:fuelFlowLimit rdf:type owl:DatatypeProperty ;
               rdfs:domain :Engine ;
               rdfs:range xsd:decimal ;
               rdfs:label "Flow rate limit of the fuel, expressed in cubic metres per second at standard temperature and pressure" .


###  http://example.org/formula1#happeningDay
:happeningDay rdf:type owl:DatatypeProperty ;
              rdfs:domain :Event ;
              rdfs:range xsd:dateTime ;
              rdfs:label "The date and time of a specific Event" .


###  http://example.org/formula1#height
:height rdf:type owl:DatatypeProperty ;
        rdfs:domain :RacingDriver ;
        rdfs:range xsd:decimal ;
        rdfs:label "The height in metres of a Driver" .


###  http://example.org/formula1#location
:location rdf:type owl:DatatypeProperty ;
          rdfs:domain :Circuit ;
          rdfs:range xsd:string ;
          rdfs:label "The name of a racing circuit" .


###  http://example.org/formula1#model
:model rdf:type owl:DatatypeProperty ;
       rdfs:domain :Car ;
       rdfs:range xsd:string ;
       rdfs:label "The specific model of a Car" .


###  http://example.org/formula1#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain :Person ;
      rdfs:range xsd:string ;
      rdfs:label "The name of a Person" .


###  http://example.org/formula1#nationality
:nationality rdf:type owl:DatatypeProperty ;
             rdfs:domain :Person ;
             rdfs:range xsd:string ;
             rdfs:label "Indicates the nationality of a person" .


###  http://example.org/formula1#organization
:organization rdf:type owl:DatatypeProperty ;
              rdfs:domain :Company ;
              rdfs:range xsd:string ;
              rdfs:label "The corporate name of a company" .


###  http://example.org/formula1#salary
:salary rdf:type owl:DatatypeProperty ;
        rdfs:domain :RacingDriver ;
        rdfs:range xsd:decimal ;
        rdfs:label "The salary of a Racing Driver in millions of euros" .


###  http://example.org/formula1#season
:season rdf:type owl:DatatypeProperty ;
        rdfs:domain :Championship ;
        rdfs:range xsd:integer ;
        rdfs:label "Indicates the year of the F1 World Championship" .


###  http://example.org/formula1#seriousAccident
:seriousAccident rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :accidentDescription ;
                 rdfs:domain :Accident ;
                 rdfs:range xsd:string ;
                 rdfs:label "Indicates if a racing accident is serious" .


###  http://example.org/formula1#sex
:sex rdf:type owl:DatatypeProperty ;
     rdfs:domain :Person ;
     rdfs:range xsd:string ;
     rdfs:label "The sex of a Person" .


###  http://example.org/formula1#team_name
:team_name rdf:type owl:DatatypeProperty ;
           rdfs:domain :Team ;
           rdfs:range xsd:string ;
           rdfs:label "The full name of a F1 Team" .


###  http://example.org/formula1#temperature
:temperature rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :weatherCondition ;
             rdfs:domain :Event ;
             rdfs:range xsd:string ;
             rdfs:label "The temperature in Celcius for a F1 Event" .


###  http://example.org/formula1#ticketLevel
:ticketLevel rdf:type owl:DatatypeProperty ;
             rdfs:comment "Indicates the type of a Ticket." .


###  http://example.org/formula1#weatherCondition
:weatherCondition rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Event ;
                  rdfs:range xsd:string ;
                  rdfs:label "A description of the weather of a F1 Event" .


###  http://example.org/formula1#weatherSummary
:weatherSummary rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :weatherCondition ;
                rdfs:domain :Event ;
                rdfs:range xsd:string ;
                rdfs:label "A small description of weather conditions like Sunny or Wet." .


###  http://example.org/formula1#weight
:weight rdf:type owl:DatatypeProperty ;
        rdfs:domain :RacingDriver ;
        rdfs:range xsd:decimal ;
        rdfs:label "The weight of an F1 Driver in kilos" .


#################################################################
#    Classes
#################################################################

###  http://example.org/formula1#Accident
:Accident rdf:type owl:Class ;
          rdfs:subClassOf rdfs:Resource ;
          rdfs:label "An accident happened in Formula 1" .


###  http://example.org/formula1#BritishDrivers
:BritishDrivers rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :nationality ;
                                      owl:hasValue "United Kingdom"
                                    ] ;
                rdfs:subClassOf :RacingDriver ;
                rdfs:label "Racing Drivers that are from United Kindgom." .


###  http://example.org/formula1#BritishWorldChampion
:BritishWorldChampion rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( :BritishDrivers
                                                                 :WorldChampion
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:subClassOf :BritishDrivers ;
                      rdfs:label "Racing Drivers that are from United Kingdom and have achieved a World Championship." .


###  http://example.org/formula1#Cameraman
:Cameraman rdf:type owl:Class ;
           rdfs:subClassOf :PressMember ;
           owl:disjointWith :Journalist ;
           rdfs:label "Cameraman of a broadcast channel" .


###  http://example.org/formula1#Car
:Car rdf:type owl:Class ;
     rdfs:subClassOf rdfs:Resource ;
     rdfs:label "Any type of Car in F1" .


###  http://example.org/formula1#ChampionTeamPrincipal
:ChampionTeamPrincipal rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty :is_managing ;
                                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass :ConstructorWorldChampion
                                           ] ;
                       rdfs:subClassOf :TeamPrincipal ;
                       rdfs:label "Team Principals that have won a Constructors World Championship" .


###  http://example.org/formula1#Championship
:Championship rdf:type owl:Class ;
              rdfs:subClassOf rdfs:Resource ,
                              [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                       owl:onProperty :consists_of ;
                                                       owl:allValuesFrom :RaceWeekend
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :consists_of ;
                                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass :RaceWeekend
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
              rdfs:label "Formula 1 Year Championship" .


###  http://example.org/formula1#Channel
:Channel rdf:type owl:Class ;
         rdfs:subClassOf rdfs:Resource ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasJournalist ;
                           owl:allValuesFrom :PressMember
                         ] ;
         rdfs:label "Formula 1 Broadcast Channel" .


###  http://example.org/formula1#Circuit
:Circuit rdf:type owl:Class ;
         rdfs:subClassOf rdfs:Resource ;
         rdfs:label "Circuit that F1 Race takes place" .


###  http://example.org/formula1#Company
:Company rdf:type owl:Class ;
         rdfs:subClassOf rdfs:Resource ,
                         [ rdf:type owl:Class ;
                           owl:unionOf ( [ rdf:type owl:Restriction ;
                                           owl:onProperty :organises ;
                                           owl:allValuesFrom :RaceWeekend
                                         ]
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :sponsors ;
                                           owl:allValuesFrom :Team
                                         ]
                                       )
                         ] ;
         rdfs:label "A third party Company that has some commercial/financial relationship with Formula 1." .


###  http://example.org/formula1#ConstructorWorldChampion
:ConstructorWorldChampion rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Restriction ;
                                                owl:onProperty :constructor_champion ;
                                                owl:someValuesFrom :Championship
                                              ] ;
                          rdfs:subClassOf :Team ;
                          rdfs:label "A team that have achieved a Constructors World Championship." .


###  http://example.org/formula1#Current
:Current rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :raced_in ;
                               owl:hasValue :season2023
                             ] ;
         rdfs:subClassOf :RacingDriver ;
         rdfs:label "Racing Drivers that race in the current season of Formula 1." .


###  http://example.org/formula1#CurrentBoard
:CurrentBoard rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :board_year ;
                                    owl:hasValue 2023
                                  ] ;
              rdfs:subClassOf :FIA ;
              rdfs:label "The board of FIA for the current season of Formula 1." .


###  http://example.org/formula1#DriversWithAccident
:DriversWithAccident rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :hadAccident ;
                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :Accident
                                         ] ;
                     rdfs:subClassOf :RacingDriver ;
                     rdfs:label "Racing Drivers that have at least a racing Accident." .


###  http://example.org/formula1#Engine
:Engine rdf:type owl:Class ;
        rdfs:subClassOf rdfs:Resource ;
        rdfs:label "Car Engine" .


###  http://example.org/formula1#Engineer
:Engineer rdf:type owl:Class ;
          rdfs:subClassOf :TeamMember ;
          owl:disjointWith :Mechanic ,
                           :RacingDriver ,
                           :TeamPrincipal ;
          rdfs:label "Engineer of a Formula 1 Team" .


###  http://example.org/formula1#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf rdfs:Resource ;
       rdfs:label "Formula 1 Grand Prix Event" .


###  http://example.org/formula1#ExperiencedViewer
:ExperiencedViewer rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty :attends ;
                                         owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                         owl:onClass :RaceWeekend
                                       ] ;
                   rdfs:subClassOf :Viewer ;
                   rdfs:label "Viewers that have attended at least 3 Formula 1 Grand Prix." .


###  http://example.org/formula1#FIA
:FIA rdf:type owl:Class ;
     rdfs:subClassOf rdfs:Resource ;
     rdfs:comment "An object of FIA represents the FIA board of one Formula 1 Season." ;
     rdfs:label "Federation Internationale de l'Automobile (Organization) Board" .


###  http://example.org/formula1#FIACars
:FIACars rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:unionOf ( :MedicalCar
                                             :SafetyCar
                                           )
                             ] ;
         rdfs:subClassOf :Car ;
         owl:disjointWith :RacingCar ;
         rdfs:label "Cars that are official for the FIA." .


###  http://example.org/formula1#FirstPlace
:FirstPlace rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:unionOf ( :Pole
                                                :Win
                                              )
                                ] ;
            rdfs:subClassOf :PodiumPlace ;
            owl:disjointWith :SecondPlace ,
                             :ThirdPlace ;
            rdfs:label "First Place in an F1 Event" .


###  http://example.org/formula1#General
:General rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :ticketLevel ;
                               owl:hasValue "GeneralAdmission"
                             ] ;
         rdfs:subClassOf :Ticket ;
         owl:disjointWith :Paddock ,
                          :VIP ;
         rdfs:label "General Admission tickets for a Formula 1 Grand Prix." .


###  http://example.org/formula1#GrandPrixSponsors
:GrandPrixSponsors rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty :organises ;
                                         owl:someValuesFrom :RaceWeekend
                                       ] ;
                   rdfs:subClassOf :Company ;
                   rdfs:label "Sponsor Companies that sponsor a Grand Prix" .


###  http://example.org/formula1#GrandSlamer
:GrandSlamer rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :PoleSitter
                                                        :Winner
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :PodiumAchiever ;
             rdfs:label "Racing Drivers that have both a Win and a Pole" .


###  http://example.org/formula1#Journalist
:Journalist rdf:type owl:Class ;
            rdfs:subClassOf :PressMember ;
            rdfs:label "Journalist of a broadcast channel" .


###  http://example.org/formula1#Mechanic
:Mechanic rdf:type owl:Class ;
          rdfs:subClassOf :TeamMember ;
          owl:disjointWith :RacingDriver ,
                           :TeamPrincipal ;
          rdfs:label "Member of the pit crew" .


###  http://example.org/formula1#MedicalCar
:MedicalCar rdf:type owl:Class ;
            rdfs:subClassOf :FIACars ;
            owl:disjointWith :SafetyCar ;
            rdfs:label "Formula 1 Medical Car" .


###  http://example.org/formula1#MultiSponsors
:MultiSponsors rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( :GrandPrixSponsors
                                                          :TeamSponsors
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf :Company ;
               rdfs:label "Sponsor Company that sponsor a Team and a Grand Prix." .


###  http://example.org/formula1#MultiWorldChampion
:MultiWorldChampion rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :driver_champion ;
                                          owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                          owl:onClass :Championship
                                        ] ;
                    rdfs:subClassOf :WorldChampion ;
                    rdfs:label "Racing Drivers that have won more than one Formula 1 World Championship." .


###  http://example.org/formula1#Paddock
:Paddock rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :ticketLevel ;
                               owl:hasValue "Paddock"
                             ] ;
         rdfs:subClassOf :Ticket ;
         owl:disjointWith :VIP ;
         rdfs:label "Paddock Pass for a Formula 1 Grand Prix." .


###  http://example.org/formula1#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf rdfs:Resource ;
        rdfs:label "Human in Formula 1" .


###  http://example.org/formula1#PodiumAchiever
:PodiumAchiever rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :achievesPodium ;
                                      owl:someValuesFrom :PodiumPlace
                                    ] ;
                rdfs:subClassOf :RacingDriver ;
                rdfs:label "Racing Drivers that have achieved at least one Podium (either Qualifying or Race) in their carreer." .


###  http://example.org/formula1#PodiumPlace
:PodiumPlace rdf:type owl:Class ;
             rdfs:subClassOf rdfs:Resource ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :achieved_by ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :RacingDriver
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :achieved_in ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Event
                             ] ;
             rdfs:label "A Podium Finish in a Grand Prix" .


###  http://example.org/formula1#Pole
:Pole rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :FirstPlace
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :achieved_in ;
                                                   owl:someValuesFrom :Qualifying
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:subClassOf :FirstPlace .


###  http://example.org/formula1#PoleSitter
:PoleSitter rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :achievesPodium ;
                                  owl:someValuesFrom :Pole
                                ] ;
            rdfs:subClassOf :PodiumAchiever ;
            rdfs:label "Racing Drivers that have won a Pole" .


###  http://example.org/formula1#PracticeSession
:PracticeSession rdf:type owl:Class ;
                 rdfs:subClassOf :Event ;
                 owl:disjointWith :Qualifying ,
                                  :Race ;
                 rdfs:label "Grand Prix Practice Session" .


###  http://example.org/formula1#PremiumViewer
:PremiumViewer rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                            owl:onProperty :buysTicket ;
                                                            owl:allValuesFrom :VIP
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :buysTicket ;
                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass :VIP
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf :Viewer ;
               rdfs:label "Viewers that buy only VIP tickets for Formula 1 Grand Prix." .


###  http://example.org/formula1#PressMember
:PressMember rdf:type owl:Class ;
             rdfs:subClassOf :Person ,
                             [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty :broadcasts_for ;
                                                      owl:allValuesFrom :Channel
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :broadcasts_for ;
                                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :Channel
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
             owl:disjointWith :RaceOfficial ,
                              :TeamMember ,
                              :Viewer ;
             rdfs:label "Member of a broadcast channel" .


###  http://example.org/formula1#Qualifying
:Qualifying rdf:type owl:Class ;
            rdfs:subClassOf :Event ;
            owl:disjointWith :Race ;
            rdfs:label "Grand Prix Qualifying Session" .


###  http://example.org/formula1#Race
:Race rdf:type owl:Class ;
      rdfs:subClassOf :Event ;
      rdfs:label "Grand Prix Race" .


###  http://example.org/formula1#RaceDirector
:RaceDirector rdf:type owl:Class ;
              rdfs:subClassOf :RaceOfficial ;
              owl:disjointWith :Steward ;
              rdfs:label "Race Director of FIA" .


###  http://example.org/formula1#RaceEngineer
:RaceEngineer rdf:type owl:Class ;
              rdfs:subClassOf :Engineer ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :communicates_with ;
                                owl:allValuesFrom :RacingDriver
                              ] ;
              owl:disjointWith :Strategist ;
              rdfs:label "Engineer of a Formula 1 Team" .


###  http://example.org/formula1#RaceOfficial
:RaceOfficial rdf:type owl:Class ;
              rdfs:subClassOf :Person ,
                              [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                       owl:onProperty :is_official ;
                                                       owl:allValuesFrom :FIA
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :is_official ;
                                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass :FIA
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
              owl:disjointWith :TeamMember ,
                               :Viewer ;
              rdfs:label "Member of a Grand Prix or FIA" .


###  http://example.org/formula1#RaceWeekend
:RaceWeekend rdf:type owl:Class ;
             rdfs:subClassOf rdfs:Resource ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :takes_place ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Circuit
                             ] ;
             rdfs:label "A F1 Grand Prix" .


###  http://example.org/formula1#RacingCar
:RacingCar rdf:type owl:Class ;
           rdfs:subClassOf :Car ,
                           [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty :developedBy ;
                                                    owl:allValuesFrom :Team
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :developedBy ;
                                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :Team
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
           rdfs:label "Formula 1 Car" .


###  http://example.org/formula1#RacingDriver
:RacingDriver rdf:type owl:Class ;
              rdfs:subClassOf :TeamMember ;
              owl:disjointWith :TeamPrincipal ;
              rdfs:label "Formula 1 Driver" .


###  http://example.org/formula1#SafetyCar
:SafetyCar rdf:type owl:Class ;
           rdfs:subClassOf :FIACars ;
           rdfs:label "Formula 1 Safety Car" .


###  http://example.org/formula1#SecondPlace
:SecondPlace rdf:type owl:Class ;
             rdfs:subClassOf :PodiumPlace ;
             rdfs:label "Second Place in an F1 Event" .


###  http://example.org/formula1#SeriousAccident
:SeriousAccident rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :seriousAccident ;
                                       owl:someValuesFrom xsd:string
                                     ] ;
                 rdfs:subClassOf :Accident ;
                 rdfs:comment "Accidents in Formula 1 that have a serious impact." .


###  http://example.org/formula1#SixCylinder
:SixCylinder rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :cylinders ;
                                   owl:hasValue 6
                                 ] ;
             rdfs:subClassOf :Engine .


###  http://example.org/formula1#SponsoredTeams
:SponsoredTeams rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :isSponsoredBy ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :Company
                                    ] ;
                rdfs:subClassOf :Team ;
                rdfs:label "Formula 1 Teams that have a Sponsorship from a third party Company." .


###  http://example.org/formula1#Steward
:Steward rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty :board_member ;
                                                      owl:allValuesFrom :RaceWeekend
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :board_member ;
                                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass :RaceWeekend
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf :RaceOfficial ;
         rdfs:label "Steward of a Grand Prix" .


###  http://example.org/formula1#Strategist
:Strategist rdf:type owl:Class ;
            rdfs:subClassOf :Engineer ;
            rdfs:label "Member of the strategist team" .


